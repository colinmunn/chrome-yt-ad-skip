{
  "name": "cm-yt-ad-skipped",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "cm_popup.html",
    "default_icon": "cm_logo.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "*://*.youtube.com/*"

      ],
      "run_at": "document_end"
    }
  ]
}
